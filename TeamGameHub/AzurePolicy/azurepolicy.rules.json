{
    "if": {
       "allOf": [
          {
             "field": "type",
             "equals": "Microsoft.SQL/servers/databases"
          },
          {
             "field": "name",
             "notEquals": "master"
          },
          {
             "not": {
                "anyOf": [
                   {
                      "field": "Microsoft.SQL/servers/databases/sku.tier",
                      "in": "[parameters('listOfSKUName')]"
                   }
                ]
             }
          }
       ]
    },
    "then": {
       "effect": "deny"
    }
 }